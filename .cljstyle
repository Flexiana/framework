{:rules
 {:indentation
  {:indents
   {pc/defmutation            [[:inner 0]]
    pc/defresolver            [[:inner 0]]
    pc/mutation               [[:inner 0]]
    pc/resolver               [[:inner 0]]
    pcg/parser-item           [[:inner 0]]
    pco/defresolver           [[:inner 0]]
    pco/resolver              [[:inner 0]]
    pcr/all-requires-ready?   [[:inner 0]]
    pcr/merge-mutation-stats! [[:inner 0]]
    pcr/merge-node-stats!     [[:inner 0]]
    prop/for-all              [[:inner 0]]
    psm/smart-map             [[:inner 0]]
    pt/trace-leave            [[:inner 0]]
    pt/tracing                [[:inner 0]]
    letfn                     [[:inner 2 0]]
    reduce                    [[:inner 0]]
    every?                    [[:inner 0]]
    conj                      [[:inner 0]]
    as->                      [[:inner 0]]
    assoc-in                  [[:inner 0]]
    some                      [[:inner 0]]
    cond->                    [[:inner 0]]
    update                    [[:inner 0]]
    update-in                 [[:inner 0]]
    recur                     [[:inner 0]]
    swap!                     [[:inner 0]]
    assert                    [[:inner 0]]
    mapv                      [[:inner 0]]
    apply                     [[:inner 0]]
    assoc                     [[:inner 0]]
    defrecord                 [[:inner 0]]
    .then                     [[:inner 0]]}}
  :whitespace  {:remove-surrounding? true
                :remove-trailing?    true
                :insert-missing?     true}
  :blank-lines {:padding-lines 1}
  :types       {:enabled? false}
  :namespaces  {:enabled?    true
                :indent-size 2}}
 :max-blank-lines 1}
