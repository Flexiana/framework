{:rules
 {:whitespace   {:remove-surrounding? true
                 :remove-trailing?    true
                 :insert-missing?     true}
  :blank-lines  {:padding-lines     1
                 :max-consecutive   1
                 :insert-padding    true
                 :trim-consecutive? true}
  :types        {:enabled? false}
  :functions    {:enabled? false}
  :eof-new-line {:enabled? true}
  :namespaces   {:enabled?    true
                 :break-libs true
                 :indent-size 2}}}
